{
  "name": "@akfm/test-utils",
  "version": "0.0.2",
  "repository": "https://github.com/AkifumiSato/akfm-test-utils",
  "description": "Utility for test by @akfm.",
  "files": ["dist", "package.json"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "test": "vitest",
    "check": "biome check .",
    "type-check": "tsc"
  },
  "keywords": ["jest", "vitest", "test", "utility"],
  "author": "akfm",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "vitest": "3.0.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
